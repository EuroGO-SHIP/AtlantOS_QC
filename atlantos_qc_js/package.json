{
    "name": "atlantos-qc",
    "description": "AtlantOS Quality Control",
    "homepage": "https://github.com/EuroGO-SHIP/AtlantOS_QC",
    "main": "main.js",
    "version": "1.7.0",
    "scripts": {
        "start": "electron .",
        "start_offline": "electron --offline .",
        "start:debug": "electron --remote-debugging-port=9223 .",
        "trace_deprecation": "electron --trace-deprecation .",
        "debug": "electron --inspect-brk=9229 .",
        "clean": "node ./src/scripts/clean.js",
        "pack": "electron-builder --dir",
        "dist_mac": "electron-builder --macos",
        "dist_win": "electron-builder --win --x64",
        "dist_linux": "electron-builder --linux --x64",
        "install_chrome": "node ./src/scripts/install_chrome.js",
        "postinstall": "node ./src/scripts/install_chrome.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/EuroGO-SHIP/AtlantOS_QC"
    },
    "publish": {
        "provider": "github"
    },
    "engines": {
        "node": ">=18.17.0",
        "npm": ">=10.3.0",
        "yarn": ">=4.9.4"
    },
    "keywords": [
        "atlantos",
        "quality",
        "whp-exchange",
        "ocean",
        "seawater",
        "bokeh"
    ],
    "author": "IIM-CSIC <avelo@iim.csic.es>",
    "license": "GPL-3.0-or-later",
    "dependencies": {
        "@electron/remote": "^2.1.3",
        "@logdna/tail-file": "^3.0.1",
        "app-module-path": "^2.2.0",
        "awesome-bootstrap-checkbox": "^1.0.1",
        "bootstrap": "^4.6.2",
        "codemirror": "^5.65.20",
        "cross-zip": "^4.0.1",
        "csv-parse": "^4.16.3",
        "datatables.net-bs4": "^1.13.11",
        "datatables.net-colreorder-bs4": "^1.7.2",
        "datatables.net-fixedheader-bs4": "^3.4.1",
        "dotenv": "^6.2.0",
        "electron-is-dev": "^1.2.0",
        "electron-updater": "^4.6.5",
        "file-url": "^2.0.2",
        "font-awesome": "^4.7.0",
        "jquery": "^3.7.1",
        "mime-db": "^1.54.0",
        "mime-type": "^3.1.0",
        "node-watch": "^0.7.4",
        "popper.js": "^1.16.1",
        "portscanner": "^2.2.0",
        "python-shell": "^5.0.0",
        "split2": "^4.2.0",
        "tabulator-tables": "^6.3.1",
        "url": "^0.11.4",
        "winston": "3.17.0",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "electron": "^31.7.7",
        "electron-builder": "^24.13.3",
        "electron-debug": "^2.2.0"
    },
    "build": {
        "appId": "atlantos.qc",
        "productName": "AtlantosQC",
        "copyright": "Copyright Â© 2019 ${author}",
        "files": [
            "**/*",
            "src/img/*",
            "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
            "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
            "!**/node_modules/*.d.ts",
            "!**/node_modules/.bin",
            "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
            "!.editorconfig",
            "!**/._*",
            "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
            "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
            "!**/{appveyor.yml,.travis.yml,circle.yml}",
            "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
        ],
        "extraResources": [
            {
                "from": "../env",
                "to": "env",
                "filter": [
                    "**/*",
                    "!*.pyc",
                    "!conda-meta${/*}"
                ]
            },
            {
                "from": "src/browser/chrome",
                "to": "src/browser/chrome",
                "filter": [
                    "**/*"
                ]
            },
            {
                "from": "src/browser/chromedriver",
                "to": "src/browser/chromedriver",
                "filter": [
                    "**/*"
                ]
            }
        ],
        "asar": false,
        "directories": {
            "buildResources": "src/img",
            "output": "dist"
        },
        "fileAssociations": [
            {
                "ext": "aqc",
                "name": "AtlantosQC File",
                "description": "AtlantosQC File",
                "icon": "icon.ico"
            }
        ],
        "linux": {
            "category": "Utility",
            "synopsis": "Tool for 1st Quality Control on Hydrographic Cruise Data",
            "target": [
                "deb",
                "snap",
                "rpm",
                "appImage"
            ]
        },
        "dmg": {
            "contents": [
                {
                    "x": 110,
                    "y": 150
                },
                {
                    "x": 240,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                }
            ]
        }
    },
    "packageManager": "yarn@4.9.4"
}
